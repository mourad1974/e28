js';\n\nexport default {\n  name: 'RecipePage',\n  props: ['id'],\n  data: function() {\n    return {\n      product: null,\n      addAlert: false\n    };\n  },\n  mounted() {\n    app.axios\n      .get(app.config.api + '/products/' + this.id)\n\n      .then(response => {\n        this.product = response.data;\n      });\n  },\n  methods: {\n    addToCart: function(productId) {\n      let cart = new app.Cart();\n      cart.add(productId);\n      app.store.cartCount = cart.count();\n      this.addAlert = true;\n      setTimeout(() => (this.addAlert = false), 2000);\n    }\n  }\n};\n</script>\n\n<style scoped>\n</style>"]}]}