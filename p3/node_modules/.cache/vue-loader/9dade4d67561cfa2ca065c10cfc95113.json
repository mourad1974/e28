 computed: {\n    featuredProducts: function() {\n      function isMatch(product) {\n        return product.categories.includes(this);\n      }\n      return this.products.filter(isMatch, this.category);\n    }\n  }\n};\n</script>\n<style lang=\"less\" scoped>\n</style>"]}]}