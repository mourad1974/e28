.cart.count();\n    }\n  },\n\n  mounted() {\n    this.cart = new app.Cart();\n    this.items = this.cart.getItems();\n\n    this.products = app.axios\n      .get(app.config.api + 'products')\n      .then(response => (this.products = response.data));\n  }\n};\n</script>"]}]}